{
    "manifest_version": 3,
    "name": "Tab Lister",
    "version": "1.1",
    "description": "Lists all open tabs in a new tab",
    "browser_specific_settings": {
        "edge": {
            "browser_action_next_to_addressbar": true
        }
    },
    "permissions": [
        "tabs"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "action": {
        "default_title": "List Open Tabs",
        "default_icon": {
            "16": "icon16.png",
            "32": "icon32.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}