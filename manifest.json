{
    "manifest_version": 2,
    "name": "Tab Lister",
    "version": "1.0",
    "description": "Lists all open tabs in a new tab",
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "List Open Tabs"
    }
}